{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "pretty": "npx prettier --write .",
    "lint": "eslint . --ext .ts",
    "jasmine": "npm run build && jasmine-ts",
    "build": "npm run pretty && npm run lint && npx tsc",
    "start": "node dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "test": "db-migrate --env test up && ENV=test npm run jasmine && db-migrate --env test down",
    "createDB": "db-migrate --env dev db:create --config database-create.json storefront && db-migrate --env dev up",
    "dropDB": "db-migrate --env dev db:drop --config database-create.json storefront",
    "fillDB": "ENV=dev node dist/database/initDB.js",
    "createDB_TEST": "db-migrate --env test db:create --config database-create.json storefront_test",
    "dropDB_TEST": "db-migrate --env test db:drop --config database-create.json storefront_test"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "supertest": "^6.1.6",
    "node": "^14.18.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.9.1",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.4.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  }
}
